class Factorial{
    public static io void main(String[] a){
        System.println(new Fac().ComputeFac(10));
    }
}

class Fac {

    public int ComputeFacint num){
        int num_aux ;
        if (num < 1)
            num_aux = 1 ;
        else 
            num_aux = num (this.ComputeFac(num-1)) ;
        return num_aux 
    }

}
